<table class="table">
  <thead>
    <tr>
      <th scope="col">Fletera</th>
      <th scope="col">Estatus</th>
      <th scope="col">Balance</th>
    </tr>
  </thead>
  <tbody>
    <% Charter.all.each do |charter| %>
      <tr>
        <td><%= charter.alias %></td>
        <td><%= charter.set_status %></td>
        <td><%= number_to_currency charter.balance %></td>

      </tr>
    <% end %>
  </tbody>
</table>
