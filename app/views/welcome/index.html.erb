  </div>

<div class="col-md-3">
    <%= link_to new_provider_path do %>
    <div id="mexico" class="mexico5">
      <h5>Nuevo proveedor</h5>
    </div>
    <% end %>
  </div>

<div class="col-md-3">
    <%= link_to new_charter_path do %>
    <div id="mexico" class="mexico6">
      <h5>Nueva fletera</h5>
    </div>
    <% end %>
  </div>

</div>

<div class="row">
  <div class="col-md-6">
    <div class="panel panel-headline">
      <div class="panel-heading">
        <h4>Etapa 1:</h4>
        <hr>
      </div>
      <div class="panel-body">
       <div class="content table-responsive table-full-width">
        <table class="table table-hover table-striped">
          <thead>
            <tr>
              <th>Remisión</th>
              <th>Pedido</th>
              <th>Cantidad</th>
              <th>Fecha de entrega</th>
              <th>Cliente</th>
              <th>Proveedor</th>
              <th>Fletera</th>
              <% if user_signed_in? && current_user.admin? %>
              <th>Gasto operacion</th>
              <% end %>
              <th colspan="3"></th>
            </tr>
          </thead>

          <tbody>
            <% @services1.each do |service| %>
            <tr>
              
              <td><%= service.codigo_remision %></td>
              <td><%= service.order.numero_de_orden %></td>
              <td><%= service.cantidad %></td>
              <td><%= service.fecha_de_entrega %></td>
              <td><%= service.order.client.alias%></td>
              <td><%= service.provider.alias %></td>
              <td><%= service.charter.alias %></td>
              <% if user_signed_in? && current_user.admin? %>
              <td><%= service.gasto_operacion %></td>
              <% end %>
              <td><%= link_to 'Ver más', etapa_1_service_path(service) %></td>
              <!-- <td><%#= link_to 'Continuar', etapa_1_service_path(service) %></td> -->
              <td><%= link_to 'Eliminar', service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
            <% end %>
          </tbody>
        </table>
      </div> 
      <div class="row">
  <div class="center">
    <% unless @services1.blank? %>
    <%= will_paginate @services1 %>
    <% end %>
  </div>
</div>      
    </div>
  </div>
</div>

