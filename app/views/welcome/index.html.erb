<div class="row">
  <div class="col-md-6">
    <%= link_to new_order_path do %>
    <div id="mexico" class="mexico1">
      <h5>Nuevo pedido</h5>
    </div>
    <% end %>
  </div>

<div class="col-md-6">
    <%= link_to new_service_path do %>
    <div id="mexico" class="mexico2">
      <h5>Nueva remisión</h5>
    </div>
    <% end %>
  </div>
</div>


<div class="row">
  <div class="col-md-3">
    <%= link_to new_client_path do %>
    <div id="mexico" class="mexico3">
      <h5>Nuevo cliente</h5>
    </div>
    <% end %>
  </div>

<div class="col-md-3">
